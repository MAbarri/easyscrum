<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="com.sqli.easyscrum.entity.User" table="User" >

	<id name="userId" column="idPO"  >
		<generator class="native"></generator>
	</id>
	
	<property name="nom" column="Nom"></property>
	<property name="prenom" column="Prenom"></property>
	<property name="adresse" column="Adresse"></property>
	<property name="email" column="Email"></property>
	<property name="telephone" column="Telephone"></property>
	<property name="photo" column="Photo"></property>
	<property name="login" column="Login"></property>
	<property name="password" column="Password"></property>
	<property name="Speciality" column="Speciality"></property>
	<property name="type" column="type"></property>
	
</class>
 
  
</hibernate-mapping>